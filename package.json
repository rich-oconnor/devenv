{
  "repository": {
    "type": "git",
    "url": "git://github.com/rich-oconnor/devenv.git"
  },
  "name": "devenv",
  "version": "1.0.0",
  "description": "js dev environment",
  "scripts": {
    "prestart": "babel-node buildScripts/startMessage.js",
    "start": "npm-run-all --parallel open:src lint:watch test:watch start-mockapi start-doc",
    "open:src": "babel-node buildScripts/server.js",
    "test": "mocha --reporter progress buildScripts/testSetup.js \"src/**/*.test.js\"",
    "test:watch": "npm run test -- --watch",
    "lint": "esw webpack.config.* src buildScripts --color",
    "lint:watch": "npm run lint -- --watch",
    "localtunnel": "lt --port 3000",
    "start-doc": "jsdoc -c jsdoc.json",
    "generate-mock-data": "babel-node buildScripts/generateMockData",
    "prestart-mockapi": "npm run generate-mock-data",
    "start-mockapi": "json-server --watch src/api/db.json --port 3001",
    "clean-dist": "rimraf ./dist && mkdir dist",
    "prebuild": "npm-run-all clean-dist test lint",
    "build": "babel-node buildScripts/build.js",
    "postbuild": "babel-node buildScripts/distServer.js start-doc"
  },
  "author": "Rich O'Connor",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.4.7",
    "babel-loader": "^8.0.5",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "cheerio": "^1.0.0-rc.2",
    "compression": "^1.7.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.0",
    "eslint": "^5.12.1",
    "eslint-plugin-import": "^2.16.0",
    "eslint-watch": "^4.0.2",
    "exports-loader": "^0.7.0",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "faker": "^4.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jsdoc": "^3.5.5",
    "jsdom": "^13.2.0",
    "json-schema-faker": "^0.3.6",
    "json-server": "^0.14.2",
    "localtunnel": "^1.9.1",
    "lodash": "^4.17.11",
    "mini-css-extract-plugin": "^0.5.0",
    "mocha": "^5.2.0",
    "nock": "^10.0.6",
    "node-sass": "^4.11.0",
    "npm-run-all": "^4.1.5",
    "numeral": "^2.0.6",
    "open": "0.0.5",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.29.2",
    "webpack-dev-middleware": "^3.5.1",
    "webpack-hot-middleware": "^2.24.3",
    "webpack-md5-hash": "0.0.6",
    "whatwg-fetch": "^3.0.0"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.7.1",
    "bootstrap": "^4.2.1",
    "jquery": "^3.3.1",
    "popper.js": "^1.14.7"
  }
}
